{
    "name": "Videos/Images Dark Mode",
    "version": "1.0.0",
    "description": "Inverts videos and images. Most suitable for courses and learning materials that contain text that cannot be converted by other extensions.",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Videos/Images Dark Mode"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}